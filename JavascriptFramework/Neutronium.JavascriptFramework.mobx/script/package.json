{
  "name": "mobx_framework_manager",
  "version": "1.0.0",
  "description": "mobx framework manager",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "dev": "cross-env NODE_ENV=dev webpack --progress --hide-modules",
    "test": "jest"
  },
  "keywords": [
    "neutronium",
    "mobx"
  ],
  "author": "david desmaisons",
  "license": "MIT",
  "dependencies": {
    "mobx": "^3.4.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^5.1.3",
    "jest": "^22.1.2",
    "webpack": "^3.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "cjson"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}