<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Wpf="clr-namespace:Neutronium.WPF" x:Class="Neutronium.WPF.Internal.DebugTools.DebugControlNeutronium"
             x:Name="Root"
             mc:Ignorable="d" 
             d:DesignWidth="300">
    <Grid>
        <Wpf:HTMLViewControl x:Name="DebugWindow"  IsDebug="false" JavascriptUIEngine="{Binding ElementName=Root, Path=JavascriptUIEngine}"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
    </Grid>
</UserControl>
